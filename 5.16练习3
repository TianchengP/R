class(NA)
class(NaN)
class("")
mode(Inf)
mode(NA)
mode(NaN)
mode("")
str(Inf)
str(NA)
str(NaN)
str("")
storage.mode(Inf)
storage.mode(NA)
storage.mode(NaN)
storage.mode("")


